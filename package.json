{
  "name": "@saas-power-factory/express-auth-kit",
  "version": "0.0.1",
  "description": "Lightweight toolkit to add JWT-based auth and route guards to Express APIs",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "prepare": "npm run build",
    "example": "node --import tsx example/server.ts"
  },
  "keywords": [
    "express",
    "jwt",
    "authentication",
    "auth",
    "middleware",
    "express-middleware",
    "jwt-auth",
    "authorization",
    "security",
    "token"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaaS-Power-Factory/package-express-auth-kit.git"
  },
  "bugs": {
    "url": "https://github.com/SaaS-Power-Factory/package-express-auth-kit/issues"
  },
  "homepage": "https://github.com/SaaS-Power-Factory/package-express-auth-kit#readme",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.19",
    "express": "^4.18.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  }
}